{
  "name": "gatsby-source-instagram-all",
  "description": "Gatsby source plugin for fetching all your instagram media",
  "version": "5.2.4",
  "author": "Mantas Mikalauskas <mantas.codes@gmail.com>",
  "bugs": {
    "url": "https://github.com/MantasMikal/gatsby-source-instagram-all"
  },
  "homepage": "https://github.com/MantasMikal/gatsby-source-instagram-all",
  "main": "gatsby-node.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir dist",
    "watch": "babel --watch src --out-dir dist --ignore **/__tests__",
    "format": "prettier --write src/**/*.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "yarn run build",
    "prepare": "yarn run build",
    "prepack": "yarn run build"
  },
  "dependencies": {
    "gatsby-source-filesystem": "^5.2.0",
    "node-fetch": "^2.6.5",
    "query-string": "^7.1.3"
  },
  "peerDependencies": {
    "gatsby": "^5.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/node": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "prettier": "^2.8.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "source",
    "instagram",
    "api",
    "all",
    "post",
    "fetch"
  ],
  "packageManager": "yarn@3.5.0"
}
